{
  "name": "vagrant-gui",
  "version": "0.0.0",
  "description": "Frontend for Vagrant using node-webkit",
  "main": "src/index.html",
  "node-main": "src/js/node/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodewebkit"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wittnl/Vagrant-GUI.git"
  },
  "keywords": [
    "Vagrant",
    "node-webkit",
    "VirtualBox"
  ],
  "author": "Nathan Witt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wittnl/Vagrant-GUI/issues"
  },
  "homepage": "https://github.com/wittnl/Vagrant-GUI",
  "window": {
    "title": "Vagrant Box Manager",
    "icon": "src/ui/images/vagrant-icon.png",
    "toolbar": true,
    "frame": true,
    "width": 800,
    "height": 500,
    "position": "mouse",
    "min_width": 400,
    "min_height": 200,
    "max_width": 800,
    "max_height": 600
  },
  "single-instance": true,
  "devDependencies": {
    "nodewebkit": "^0.9.2-7",
    "vagrant": "0.0.1",
    "semver": "^2.3.1"
